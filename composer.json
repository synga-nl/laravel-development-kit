{
    "name": "synga/laravel-development-kit",
    "description": "Development Kit for Laravel to start your projects very easy",
    "type": "library",
    "authors": [
        {
            "name": "Synga",
            "email": "info@synga.nl"
        }
    ],
    "require": {
        "laravel/framework": ">=5.5",
        "synga/interactive-console-tester": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.10.*"
    },
    "autoload": {
        "psr-4": {
            "Synga\\LaravelDevelopment\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Synga\\LaravelDevelopment\\LaravelDevelopmentServiceProvider"
            ]
        }
    }
}